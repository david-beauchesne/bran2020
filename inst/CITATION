citHeader("To cite bran2020 in publications use:")

bibentry(
  bibtype = "Manual",
  title   = "bran2020: {An} {R} package to ...",
  author  = c(person("Beauchesne David")),
  year    = "2022",
  note    = "R package version 0.0.0.9000",
  url     = "https://github.com/david-beauchesne/bran2020"
)
